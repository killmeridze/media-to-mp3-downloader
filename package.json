  "description": "A desktop application that downloads videos from YouTube, Instagram, and TikTok, converts them to MP3. Users can insert URLs for downloads and choose the directory to save them.",
  "main": "src/main/main.ts",
  "scripts": {
    "start": "electron .",
    "build": "tsc && vite build",
    "dev": "vite",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "tailwindcss"
  ],
  "author": "killmeridze",
  "license": "MIT",
  "dependencies": {
    "electron": "latest",
    "react": "latest",
    "react-dom": "latest",
    "tailwindcss": "latest"
  },
  "devDependencies": {
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "typescript": "latest",
    "vite": "latest",
    "eslint": "latest",
    "eslint-plugin-react": "latest",
    "jest": "latest",
    "ts-jest": "latest"
  }
}
